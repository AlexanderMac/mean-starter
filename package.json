{
  "name": "mean-starter",
  "version": "0.0.1",
  "description": "MEAN-starter",
  "private": true,
  "engines": {
    "node": "^8",
    "npm": "^5"
  },
  "scripts": {
    "postinstall": "npm run install --prefix server && npm run install --prefix client",
    "start": "concurrently --kill-others \"npm start --prefix server\" \"npm run build  --prefix client\"",
    "lint": "npm run lint --prefix server && npm run lint --prefix client",
    "test": "npm run test --prefix server",
    "build": "npm run build --prefix client",
    "coverage": "npm run coverage --prefix server"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^3.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderMac/mean-starter"
  },
  "author": "Alexander Mac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexanderMac/mean-starter/issues"
   },
  "homepage": "https://github.com/AlexanderMac/mean-starter"
}
