{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "Express-starter",
  "private": true,
  "engines": {
    "node": "^8",
    "npm": "^5"
  },
  "scripts": {
    "start": "nodemon --config .nodemon.json ./src/app.js",
    "lint": "eslint -f stylish 'src/**/*.js' 'test/**/*.js'",
    "test": "NODE_ENV=test mocha",
    "coverage": "nyc npm test",
    "codecov": "npm run coverage && nyc report --reporter=text-lcov > ../coverage.lcov"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "n-custom-errors": "^2.0.1",
    "n-params-processor": "^4.0.1",
    "nconf": "^0.10.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "mocha": "^5.2.0",
    "n-assert": "^4.0.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "should": "^13.2.3",
    "sinon": "^7.2.2",
    "supertest": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderMac/express-starter"
